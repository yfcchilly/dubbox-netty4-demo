<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

    <dubbo:application name="consumer-clcc-app" owner="chilly" organization="clcc" logger="slf4j"/>

    <dubbo:registry protocol="zookeeper" address="192.168.99.100:2181"/>

    <dubbo:reference interface="com.clcc.rpc.provider.UserService" client="netty4"
                     version="1.0.0" group="daily" id="userService" />


</beans>